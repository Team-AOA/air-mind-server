{
  "name": "air-mind-server",
  "version": "0.11.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "spec": "mocha spec --sort --exit",
    "pre-commit": "lint-staged && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "prepare": "husky install && husky add .husky/pre-commit 'npm run pre-commit'"
  },
  "lint-staged": {
    "**/*.{js, jsx, html, css, json}": [
      "prettier --write .",
      "eslint --fix ."
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.2.0",
    "http-errors": "~1.6.3",
    "mongoose": "^6.6.5",
    "mongoose-autopopulate": "^0.16.1",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.0"
  }
}
